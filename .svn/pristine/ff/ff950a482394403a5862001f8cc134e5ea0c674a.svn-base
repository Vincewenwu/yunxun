package com.vision.edu.dto.apiVo;

/**
 * 之定义接送人实体
 * @author zhangfeihong
 * @date 2017年8月1日 上午9:53:35
 * @version 1.0
 */
public class GuardianPo {
	/**
	 * 接送人名称
	 */
	private String name;
	/**
	 * 接送人关系
	 */
	private String relation;
	/**
	 * 接送人头像
	 */
	private String surrogateHeadurl;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getRelation() {
		return relation;
	}

	public void setRelation(String relation) {
		this.relation = relation;
	}

	public String getSurrogateHeadurl() {

		return "http://yunxunimg.xuechenedu.com/" + surrogateHeadurl;
	}

	public void setSurrogateHeadurl(String surrogateHeadurl) {
		this.surrogateHeadurl = surrogateHeadurl;
	}

}
